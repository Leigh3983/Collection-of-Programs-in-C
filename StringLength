# Given a char pointer, the function returns the length of the string pointed to by the pointer.

#include <stddef.h>
#include <assert.h>

size_t StringLength(const char* string) 
{  
    size_t length = 0;
    
    assert(string != NULL);
    
    while(string[length] != '\0') 
    {
        length++;
    }
    
    return length;
}
